#2025.08.24
#s4

N, M = map(int, input().split())

name_to_num = {}

num_to_name = [None] * (N + 1)

for i in range(1, N + 1):
    name = input().strip()
    name_to_num[name] = i
    num_to_name[i] = name

for _ in range(M):
    pokemon= input().strip()

    if pokemon.isdigit():  # 숫자면 번호를 이름으로
        print(num_to_name[int(pokemon)])
    else:  # 문자열이면 이름을 번호로
        print(name_to_num[pokemon])